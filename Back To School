#include <bits/stdc++.h>
#define ll long long
#define PI 3.14159265358979323846
using namespace std;
int used[1000];
int main()
{
    ios::sync_with_stdio(false);cin.tie(0);cout.tie(0);

  ll t,st;
  cin>>t>>st;
  while(t--){
    ll x,y,z;
    cin>>x>>y>>z;
    if(z<y){
        cout << 0 << endl;
    }
    else{
        ll can= z/y;
        if(can>x)
            cout << x << endl;
        else if(can<x){
            cout << can << endl;
        }
        else{
            cout << can << endl;
        }
    }
  }
}
